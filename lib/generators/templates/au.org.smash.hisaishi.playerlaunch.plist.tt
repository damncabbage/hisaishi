<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>au.org.smash.hisaishi.playerlaunch</string>
  <key>ProgramArguments</key>
  <array>
    <string>sh</string>
    <string>-i</string>
    <string>-c</string>
    <string>$SHELL --login -c "'./hisaishi.sh'"</string>
  </array>
  <key>KeepAlive</key>
  <true/>
  <key>RunAtLoad</key>
  <true/>
  <key>UserName</key>
  <string>hisaishiuser</string>
  <key>WorkingDirectory</key>
	<string>/path/to/hisaishi</string>
</dict>
</plist>
